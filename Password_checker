import string

def main():
  usrinput = input("New Password: ")
  print(f"passowrd {usrinput} is {password_strength(usrinput)}")

def password_strength(pw):
  if any(c.isupper() for c in pw) and any(c.islower() for c in pw) and any(c.isdigit() for c in pw) and any(c in string.punctuation for c in pw):
    return "Strong"
  elif sum([any(c.isupper() for c in pw), any(c.islower() for c in pw), any(c.isdigit() for c in pw), any(c in string.punctuation for c in pw)]) == 3:
    return "Medium"
  else:
    return "Weak"



main()
